<%@ page language="java" contentType="text/html; charset=utf-8"
	pageEncoding="utf-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>注册页面</title>
</head>
<body>
	<h3>请填写注册信息</h3>
	<form action="${pageContext.request.contextPath}/UserInsertServlet?option=reg" 
	method="post">
	<table>
		<tr>
			<td>用户名</td>
			<td><input type="text" value="test-user" name="username"/></td>
		</tr>
		<tr>
			<td>密码</td>
			<td><input type="password" value="test-pwd" name="pwd1"/></td>
		</tr>
		<tr>
			<td>确认密码</td>
			<td><input type="password" value="test-pwd" name="pwd2"/></td>
		</tr>
		<tr>
			<td><input type="submit" value="注册"></td>
			<td><input type="reset" value="重置"></td>
		</tr>
	</table>
	</form>
</body>
</html>